<?xml version="1.0" encoding="UTF-8"?>
        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.joinpractice.member.dto.MemberDao">
        <insert id="insertMember" parameterType="com.joinpractice.member.dto.MemberDto">
            insert into easy.member(email, password)
            values (#{email},#{password});
        </insert>

    <select id="findByEmailAndPassword" parameterType="com.joinpractice.member.dto.MemberDto"
            resultType="com.joinpractice.member.dto.MemberDto">
        SELECT id, email, password
        FROM easy.member
        WHERE email = #{email} AND password = #{password}
    </select>
</mapper>